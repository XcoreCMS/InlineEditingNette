{
  "name": "xcore/inline-editing-nette",
  "type": "library",
  "description": "Content inline editable bundle for Nette.",
  "keywords": [
    "nette",
    "xcore",
    "inline"
  ],
  "homepage": "https://appio.cz",
  "license": "MIT",
  "authors": [
    {
      "name": "Jakub Janata",
      "email": "jakubjanata@gmail.com"
    }
  ],
  "require": {
    "php": "^7.1",
    "xcore/inline-editing": "~1.2.5",
    "freezy-bee/nette-caching-psr6": "^1.1",
    "freezy-bee/prepend-route": "^1.0",
    "latte/latte": "^2.4 || ^3.0",
    "nette/application": "^2.4 || ^3.0",
    "nette/di": "^2.4 || ^3.0"
  },
  "require-dev": {
    "nette/bootstrap": "^2.4",
    "nette/security": "^2.4",
    "tracy/tracy": "^2.4",
    "kdyby/translation": "^2.3",
    "kdyby/doctrine": "^3.0",
    "kdyby/validator": "^0.6",
    "symfony/property-access": "^5.0",
    "squizlabs/php_codesniffer": "^3.0",
    "nette/tester": "^2.1",
    "phpstan/phpstan": "^0.12"
  },
  "suggest": {
    "kdyby/doctrine": "If you wanna use entity features",
    "kdyby/validator": "If you wanna use entity features",
    "symfony/property-access": "If you wanna use entity features"
  },
  "autoload": {
    "psr-4": {
      "XcoreCMS\\InlineEditingNette\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "XcoreCMS\\InlineEditingNette\\Tests\\": "tests/"
    }
  },
  "prefer-stable": true,
  "config": {
    "discard-changes": true
  }
}
